{
  "rewrites": [
    {
      "source": "/kulinare_pdf/(.*)",
      "destination": "/kulinare_pdf/$1"
    },
    {
      "source": "/kulinare_images/(.*)",
      "destination": "/kulinare_images/$1"
    },
    {
      "source": "/images/(.*)",
      "destination": "/images/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
